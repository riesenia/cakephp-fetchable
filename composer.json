{
    "name": "riesenia/cakephp-fetchable",
    "description": "CakePHP ORM plugin for fetching entities from cache",
    "keywords": ["cakephp", "orm", "fetch", "cache"],
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomas Saghy",
            "email": "segy@riesenia.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "cakephp/orm": "^4.0"
    },
    "require-dev": {
        "cakephp/cakephp": "^4.0",
        "friendsofphp/php-cs-fixer": "~3.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^8.5|^9.3",
        "rshop/php-cs-fixer-config": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Fetchable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fetchable\\Test\\": "tests",
            "TestApp\\": "tests/test_app/TestApp",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
